extends layout

block content
	h1 Log In
	p Please login below
	form(method='post', action='/users/login')
		.form-group
			label Username
			input.form-control(name='username', type='text')
		.form-group
			label Password
			input.form-control(name='password', type='password')
		.form-group
			input.btn.btn-primary(name='submit', type='submit', value='Login')